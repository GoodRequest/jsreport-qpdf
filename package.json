{
  "name": "custom-extension",
  "version": "0.0.0",
  "description": "jsreport studio extension starter kit",
  "keywords": [
    "jsreport"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "test": "standard",
    "start": "jsreport-studio-start --store.provider=fs --httpPort 5488 --allowLocalFilesAccess=true --blobStorage.provider=fs --blobStorage.dataDirectory=files",
    "prepublish": "jsreport-studio-build"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "dependencies": {
    "@jsreport/pdfjs": "0.6.0",
    "cheerio": "0.22.0",
    "lodash.omit": "4.5.0",
    "nanoid": "3.1.12",
    "node-forge": "0.10.0",
    "node-signpdf": "1.3.2",
    "node.extend": "2.0.2",
    "parse-color": "1.0.0",
    "pdfjs-dist": "2.0.489",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jsreport/jsreport-assets": "3.0.0",
    "@jsreport/jsreport-child-templates": "3.0.0",
    "@jsreport/jsreport-chrome-pdf": "3.0.0",
    "@jsreport/jsreport-core": "3.0.1",
    "@jsreport/jsreport-handlebars": "3.0.0",
    "@jsreport/jsreport-jsrender": "3.0.0",
    "@jsreport/jsreport-scripts": "3.0.0",
    "@jsreport/studio-dev": "3.0.1",
    "handlebars": "4.7.7",
    "mocha": "8.2.1",
    "puppeteer": "10.4.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "16.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
